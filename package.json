  "scripts": {
    "test": "cross-env NODE_ENV=test knex migrate:latest --env test && mocha test/**/**/*.spec.js --exit",
    "dev": "nodemon index.js",
    "start": "knex migrate:latest && node index.js"
  },
  "repository": {
    "type": "git",
