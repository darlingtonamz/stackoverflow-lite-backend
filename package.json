  },
  "homepage": "https://github.com/darlingtonamz/stackoverflow-lite-backend#readme",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "chance": "^1.0.16",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "pg": "^7.4.3",
    "pg-promise": "^8.4.6",
    "swagger-ui-express": "^4.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
